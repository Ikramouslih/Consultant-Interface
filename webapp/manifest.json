{
  "_version": "1.28.0",
  "sap.app": {
    "id": "management",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.13.2",
      "toolsId": "7ed36779-f4c8-44ff-b325-8890e472d701"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZODA_GEST_DISPO_SRV_02/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.84.44",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {},
        "sap.ui.layout": {},
				"sap.uxap": {},
        "sap.suite.ui.microchart": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "management.i18n.i18n",
          "async": true,
					"fallbackLocale": "en",
					"supportedLocales": ["en", "de"],
					"terminologies": {
						"sports": {
							"bundleUrl": "i18n/terminologies/sports/i18n.terminologies.sports.properties",
							"bundleUrlRelativeTo": "manifest",
							"supportedLocales": ["en", "de"]
						},
						"travel": {
							"bundleUrl": "i18n/terminologies/travel/i18n.terminologies.travel.properties",
							"bundleUrlRelativeTo": "manifest",
							"supportedLocales": ["en", "de"]
						}
					},
					"enhanceWith": [
						{
							"bundleUrl": "reuse/appvar1/i18n/i18n.properties",
							"fallbackLocale": "en",
							"supportedLocales": [
								"en", "de"
							],
							"terminologies": {
								"sports": {
									"bundleUrl": "reuse/appvar1/i18n/terminologies/sports/i18n.terminologies.sports.properties",
									"bundleUrlRelativeTo": "manifest",
									"supportedLocales": [
										"en", "de"
									]
								},
								"travel": {
									"bundleUrl": "reuse/appvar1/i18n/terminologies/travel/i18n.terminologies.travel.properties",
									"bundleUrlRelativeTo": "manifest",
									"supportedLocales": [
										"en", "de"
									]
								}
							}
						},
						{
							"bundleUrl": "reuse/appvar2/i18n/i18n.properties",
							"bundleUrlRelativeTo": "manifest",
							"supportedLocales": [
								"en",
								"de"
							],
							"fallbackLocale": "en",
							"terminologies": {
								"travel": {
									"bundleUrl": "reuse/appvar2/i18n/terminologies/travel/i18n.terminologies.travel.properties",
									"bundleUrlRelativeTo": "manifest",
									"supportedLocales": [
										"en", "de"
									]
								}
							}
						}
					]
				}
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      }
    },
    "side": {
      "type": "sap.ui.model.json.JSONModel",
      "uri": "model/sideContent.json"
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "management.view",
        "controlAggregation": "pages",
        "controlId": "mainContents",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteManagement",
          "pattern": ":?query:",
          "target": [
            "TargetManagement"
          ]
        },
        {
          "name": "RouteConsultant",
          "pattern": "Consultants/",
          "target": [
            "TargetConsultants"
          ]
        },
        {
          "name": "RouteTicket",
          "pattern": "Tickets/",
          "target": [
            "TargetTicket"
          ]
        },
        {
          "name": "ConsultantDetails",
          "pattern": "consultantDetails/{consultantId}",
          "target": [
            "ConsultantDetails"
          ]
        },
        {
          "name": "CreateTicket",
          "pattern": "CreateTicket/",
          "target": [
            "CreateTicket"
          ]
        } 
      ],
      "targets": {
        "TargetManagement": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Management",
          "viewName": "Management"
        },
        "TargetConsultants": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Consultants",
          "viewName": "Consultants"
        },
        "TargetTicket": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Tickets",
          "viewName": "Tickets"
        },
        "ConsultantDetails": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "ConsultantDetails",
          "viewName": "Dashboard.ConsultantDetails"
        },
        "CreateTicket": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "CreateTicket",
          "viewName": "Ticketform"
        }
      }
    },
    "rootView": {
      "viewName": "management.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
