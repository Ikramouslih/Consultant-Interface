{
  "_version": "1.28.0",
  "sap.app": {
    "id": "management",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.13.2",
      "toolsId": "7ed36779-f4c8-44ff-b325-8890e472d701"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZODA_GEST_DISPON_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      },
      "Service1": {
        "uri": "/sap/opu/odata/sap/ZODA_GEST_CALENDRIER_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "supportedThemes": [
      "sap_hcb",
      "sap_hcw",
      "sap_horizon"
    ]
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.84.44",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {},
        "sap.ui.layout": {},
        "sap.uxap": {},
        "sap.suite.ui.microchart": {},
        "sap.suite.ui.commons": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "management.i18n.i18n",
          "async": true,
          "fallbackLocale": "en",
          "supportedLocales": [
            "en",
            "de"
          ],
          "terminologies": {
            "sports": {
              "bundleUrl": "i18n/terminologies/sports/i18n.terminologies.sports.properties",
              "bundleUrlRelativeTo": "manifest",
              "supportedLocales": [
                "en",
                "de"
              ]
            },
            "travel": {
              "bundleUrl": "i18n/terminologies/travel/i18n.terminologies.travel.properties",
              "bundleUrlRelativeTo": "manifest",
              "supportedLocales": [
                "en",
                "de"
              ]
            }
          },
          "enhanceWith": [
            {
              "bundleUrl": "reuse/appvar1/i18n/i18n.properties",
              "fallbackLocale": "en",
              "supportedLocales": [
                "en",
                "de"
              ],
              "terminologies": {
                "sports": {
                  "bundleUrl": "reuse/appvar1/i18n/terminologies/sports/i18n.terminologies.sports.properties",
                  "bundleUrlRelativeTo": "manifest",
                  "supportedLocales": [
                    "en",
                    "de"
                  ]
                },
                "travel": {
                  "bundleUrl": "reuse/appvar1/i18n/terminologies/travel/i18n.terminologies.travel.properties",
                  "bundleUrlRelativeTo": "manifest",
                  "supportedLocales": [
                    "en",
                    "de"
                  ]
                }
              }
            },
            {
              "bundleUrl": "reuse/appvar2/i18n/i18n.properties",
              "bundleUrlRelativeTo": "manifest",
              "supportedLocales": [
                "en",
                "de"
              ],
              "fallbackLocale": "en",
              "terminologies": {
                "travel": {
                  "bundleUrl": "reuse/appvar2/i18n/terminologies/travel/i18n.terminologies.travel.properties",
                  "bundleUrlRelativeTo": "manifest",
                  "supportedLocales": [
                    "en",
                    "de"
                  ]
                }
              }
            }
          ]
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {}
      },
      "Service1": {
        "dataSource": "Service1",
        "preload": true,
        "settings": {}
      }

    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "management.view",
        "controlAggregation": "pages",
        "controlId": "mainContents",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteManagement",
          "pattern": ":?query:",
          "target": [
            "TargetManagement"
          ]
        },
        {
          "name": "RouteConsultant",
          "pattern": "Consultants/",
          "target": [
            "TargetConsultants"
          ]
        },
        {
          "name": "RouteTicket",
          "pattern": "Tickets/",
          "target": [
            "TargetTicket"
          ]
        },
        {
          "name": "ConsultantDetails",
          "pattern": "consultantDetails/{consultantId}",
          "target": [
            "ConsultantDetails"
          ]
        },
        {
          "name": "CreateTicket",
          "pattern": "CreateTicket/",
          "target": [
            "CreateTicket"
          ]
        },
        {
          "name": "AssignTicket",
          "pattern": "AssignTicket/{IdTicket}",
          "target": [
            "AssignTicket"
          ]
        },
        {
          "name": "CreateUser",
          "pattern": "CreateUser/",
          "target": [
            "CreateUser"
          ]
        },
        {
          "pattern": "KeyNumbers",
          "name": "KeyNumbers",
          "target": [
            "KeyNumbers"
          ]
        },
        {
          "name": "Project",
          "pattern": "Project/",
          "target": [
            "Project"
          ]
        },
        {
          "name": "CreateProjects",
          "pattern": "CreateProjects/",
          "target": [
            "TargetCreateProjects"
          ]
        },
        {
          "name": "CTicket",
          "pattern": "CTicket/{consultantId}",
          "target": [
            "TargetCTicket"]
        },
        {
          "name": "UpdateConsultant",
          "pattern": "consultant/update/{consultantId}",
          "target": [
            "TargetUpdateConsultant"
          ]
        },
        {
          "name": "Managers",
          "pattern": "Managers/",
          "target": [
            "Managers"
          ]
        },
        {
          "name": "UpdateTicket",
          "pattern": "ticket/update/{ticketId}",
          "target": [
            "UpdateTicket"
          ]
        },
        {
          "name": "UpdateManager",
          "pattern": "manager/update/{managerId}",
          "target": [
            "UpdateManager"
          ]
        },
        {
          "name": "ConsultantCalendar",
          "pattern": "ConsultantCalendar/",
          "target": [
            "TargetConsultantCalendar"
          ]
        }
      ],
      "targets": {
        "TargetManagement": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Management",
          "viewName": "Management"
        },
        "TargetConsultants": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Consultants",
          "viewName": "Consultants"
        },
        "TargetTicket": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Tickets",
          "viewName": "Tickets"
        },
        "ConsultantDetails": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "ConsultantDetails",
          "viewName": "Dashboard.ConsultantDetails"
        },
        "CreateTicket": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "CreateTicket",
          "viewName": "Ticketform"
        },
        "AssignTicket": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "AssignTicket",
          "viewName": "AssignTicket"
        },
        "CreateUser": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "CreateUser",
          "viewName": "CreateUser"
        },
        "Project": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Project",
          "viewName": "Project"
        },
        "TargetCreateProjects": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "CreateProjects",
          "viewName": "ProjectCreate"
        },
        "TargetCTicket": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "CTicket",
          "viewName": "Consultant.CTicket"
        },
        "TargetUpdateConsultant": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "UpdateConsultant",
          "viewName": "UpdateConsultant"
        },
        "Managers": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "Managers",
          "viewName": "Managers"
        }
        ,
        "UpdateManager": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "UpdateManager",
          "viewName": "UpdateManager"
        },
        "UpdateTicket": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "UpdateTicket",
          "viewName": "UpdateTicket"
        },
        "TargetConsultantCalendar": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "ConsultantCalendar",
          "viewName": "Consultant.ConsultantCalendar"
        }
      }
    },
    "rootView": {
      "viewName": "management.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}