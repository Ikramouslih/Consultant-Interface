<mvc:View
		controllerName="management.controller.AssignTicket"
		 xmlns:mvc="sap.ui.core.mvc"
                xmlns="sap.m"
                xmlns:f="sap.f"
                xmlns:card="sap.f.cards"
                xmlns:core="sap.ui.core"
                displayBlock="true"
                height="100%">
<f:Card id="_IDGenCard1" class="sapUiMediumMargin" width="500px" >
        <f:content >
        <CustomListItem id="_IDGenCustomListItem1">
        <VBox id="_IDGenVBox1">
        <InputListItem id="_IDGenInputListIte1" label="Jira Ticket Id :"> 
        <Input 
                id="IdTicketJira"
                placeholder="test"
                value="{IdTicket}"
               enabled="false">
        </Input>
        </InputListItem> 
        
        <InputListItem id="_IDGenInputListIte2" label="Titre :"> 
        <Input 
                id="Titre"
                value="{Titre}"
                enabled="false">
        </Input>
        </InputListItem>
        <InputListItem id="_IDGenInputListIte3" label="Project :"> 
        <Select id="Projet"
                forceSelection="false"
                items="{
                    path: '/PROJECTIDSet',
                    sorter: {path: 'IdProject' }
                }"
                >
                <core:Item id="_IDGenItem1" key="{IdProject}" text="{NomProjet}" />
        </Select>
        </InputListItem>
                <InputListItem id="_IDGenInputListIte33" label="Consultant :"> 
        <Select id="Consultant"
                forceSelection="false"
                items="{
                    path: '/CONSULTANTIDSet',
                    sorter: {path: 'ConsultantId' }
                }"
                >
                <core:Item id="_IDGenItem2" key="{ConsultantId}" text="{Name} {FirstName}"/>
                
        </Select>
        </InputListItem>
        <InputListItem id="_IDGenInputListIte4" label="Description :"> 
        <Input 
                id="Description"
                placeholder="Enter Description "
                value="{Description}"
                enabled="false">
        </Input>
        </InputListItem>
        <InputListItem id="_IDGenInputListIte5" label="Technology :"> 
        <Input 
                id="Technology"
                placeholder="Enter Technology "
                value="{Technology}"
                enabled="false"
                >
        </Input>
        </InputListItem>
        <InputListItem id="_IDGenInputListIte6" label="Estimated :"> 
        <Input 
                id="Estimated"
                placeholder="Enter duration "
                value="{Estimated}"
                enabled="false"
                >
        </Input>
        </InputListItem>        
            <ToggleButton id="_IDGenToggleButton1" icon="sap-icon://add" press="onAssign" text="Assign Ticket"/>
            </VBox>
            </CustomListItem>
            </f:content>
            </f:Card>	
</mvc:View>
