<mvc:View
		controllerName="management.controller.ConsultantCalendar"
		xmlns:l="sap.ui.layout"
		xmlns:u="sap.ui.unified"
		xmlns:mvc="sap.ui.core.mvc"
		xmlns="sap.m"
		class="viewPadding">
	<l:HorizontalLayout id="_IDGenHorizontalLayout1" >
		<l:VerticalLayout id="_IDGenVerticalLayout1">
			<u:Calendar
					id="calendar"
					select="handleCalendarSelect"
					intervalSelection="false"
					singleSelection="false" />
			<Button id="_IDGenButton1"
					press="handleRemoveSelection"
					text="Remove All Selected Dates" />
			<List
					id="selectedDatesList"
					class="labelMarginLeft"
					noDataText="No Dates Selected"
					headerText="Selected Dates (yyyy-mm-dd)"
					items="{path: '/selectedDates'}">
				<StandardListItem id="_IDGenStandardListItem1"
						title="{Date}" />
			</List>
		</l:VerticalLayout>
		<Label id="_IDGenLabel1" text="Availability:" class="sapUiTinyMarginEnd"/> 
		<Switch id="_IDGenSwitch1" type="AcceptReject" state="{/Enabled}"/>
	</l:HorizontalLayout>
	<l:VerticalLayout id="_IDGenVerticalLayout2" >
			<Input
					id="productInput"
					type="Text"
					placeholder="Enter Ticket ..."
					showValueHelp="true"
					valueHelpRequest=".onValueHelpRequest"
					width="15rem" />
	</l:VerticalLayout>	
</mvc:View>
 