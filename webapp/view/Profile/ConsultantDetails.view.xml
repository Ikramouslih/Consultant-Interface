<mvc:View
    controllerName="management.controller.Profile.ConsultantDetails"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns:tnt="sap.tnt"
    displayBlock="true"
>
    <Page
        id="_IDGenPage1"
        class="consultantDetailsPage"
        title="Details"
    >
        <FlexBox
            id="idFlexDashboardContent"
            height="70%"
        >
            <VBox id="vboxCard">
                <mvc:XMLView
                    id="_IDGenXMLView1"
                    viewName="management.view.Profile.Card"
                />
                <layoutData>
                    <FlexItemData
                        id="_IDGenFlexItemData3"
                        growFactor="1"
                        baseSize="0"
                        backgroundDesign="Solid"
                        styleClass="sapUiTinyMargin"
                    />
                </layoutData>
            </VBox>
            <VBox id="vboxCalendar">
                <mvc:XMLView
                    id="_IDGenXMLView2"
                    viewName="management.view.Profile.Calendar"
                />
                <layoutData>
                    <FlexItemData
                        id="_IDGenFlexItemData4"
                        growFactor="1"
                        baseSize="0"
                        backgroundDesign="Solid"
                        styleClass="sapUiTinyMargin"
                    />
                </layoutData>
            </VBox>
            <VBox
                id="vboxDonut"
                alignItems="Center"
                justifyContent="Center"
                direction="Column"
                renderType="Bare"
                width="25rem"
                height="12rem"
            >
                <mvc:XMLView
                    id="_IDGenXMLView3"
                    viewName="management.view.Profile.Donut"
                    class="DonutContainer"
                />
                <layoutData>
                    <FlexItemData
                        id="_IDGenFlexItemData5"
                        growFactor="1"
                        baseSize="0"
                        backgroundDesign="Solid"
                        styleClass="sapUiTinyMargin"
                    />
                </layoutData>
            </VBox>
        </FlexBox>
        <VBox
            id="vboxTicketTable"
            class="vboxTicketTable"
        >
            <mvc:XMLView
                id="_IDGenXMLView4"
                viewName="management.view.Profile.TicketsTable"
            />
        </VBox>
    </Page>


    <ScrollContainer
        id="_IDGenScrollContainer1"
        vertical="true"
        horizontal="false"
        height="100%">
        
        <a:Card id="_IDGenCard1" class="sapUiMediumMargin" width="95%">
            <a:content>
                <VBox id="_IDGenVBox1a" class="sapUiSmallMargin" >
                    <f:Form id="FormToolbar" editable="true" ariaLabelledBy="Title1" width="103%">
                        <f:toolbar>
                            <Toolbar id="TB1">
                                <Title id="Title1" text="My Profile"/>
                                <ToolbarSpacer id="_IDGenToolbarSpacer1" />
                            </Toolbar>
                        </f:toolbar>
                        <f:layout>
                            <f:ResponsiveGridLayout id="_IDGenResponsiveGridLayout1"
                                labelSpanXL="0"
                                labelSpanL="0"
                                labelSpanM="0"
                                labelSpanS="0"
                                emptySpanXL="0"
                                emptySpanL="0"
                                emptySpanM="0"
                                emptySpanS="0"
                                columnsXL="3"
                                columnsL="3"
                                columnsM="3"
                                singleContainerFullSize="false"
                            />
                        </f:layout>
                        <f:formContainers>

                            <f:FormContainer id="_IDGenFormContainer1" ariaLabelledBy="Title2">
                                <f:toolbar>
                                    <Toolbar id="_IDGenToolbar1">
                                        <Title id="Title2" text="On Hold"/>
                                        <ToolbarSpacer id="_IDGenToolbarSpacer2" />
                                    </Toolbar>
                                </f:toolbar>
                                <f:formElements>                                    
                                    <f:FormElement id="_IDGenFormElement1">
                                        <HBox id="_IDGenHBox1" width="100%">
                                            <mvc:XMLView
                                                id="_IDGenXMLView1zed"
                                                viewName="management.view.Profile.Card"
                                            />
                                        </HBox>
                                    </f:FormElement>
                                </f:formElements>
                            </f:FormContainer>
							

                            <f:FormContainer id="_IDGenFormContainer2" ariaLabelledBy="Title3">
                                <f:toolbar>
                                    <Toolbar id="_IDGenToolbar2">
                                        <Title id="Title3" text="In Progress"/>
                                        <ToolbarSpacer id="_IDGenToolbarSpacer3" />
                                    </Toolbar>
                                </f:toolbar>
                                <f:formElements>                                    
                                    <f:FormElement id="_IDGenFormElement2">
                                        <HBox id="_IDGenHBox2" width="100%">
                                            <mvc:XMLView
                                                id="_IDGenXMLVieszszw2"
                                                viewName="management.view.Profile.Calendar"
                                            />
                                        </HBox>
                                    </f:FormElement>
                                </f:formElements>
                            </f:FormContainer>

                            <f:FormContainer id="_IDGenFormContainer3" ariaLabelledBy="Title4">
                                <f:toolbar>
                                    <Toolbar id="_IDGenToolbar3">
                                        <Title id="Title4" text="Done"/>
                                        <ToolbarSpacer id="_IDGenToolbarSpacer4" />
                                    </Toolbar>
                                </f:toolbar>
                                <f:formElements>                                    
                                    <f:FormElement id="_IDGenFormElement3">
                                        <HBox id="_IDGenHBoxzdzca2" width="100%">
                                            <mvc:XMLView
                                                id="_IDGenXMLzszszView3"
                                                viewName="management.view.Profile.Donut"
                                                class="DonutContainer"
                                            />
                                        </HBox>
                                    </f:FormElement>
                                </f:formElements>
                            </f:FormContainer>
                        </f:formContainers>
                    </f:Form>
                </VBox>
            </a:content>
        </a:Card>
    </ScrollContainer>
</mvc:View>
