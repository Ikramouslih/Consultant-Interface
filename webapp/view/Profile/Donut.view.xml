<mvc:View
    controllerName="management.controller.Profile.ConsultantDetails"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:m="sap.m"
    xmlns="sap.suite.ui.microchart"
    xmlns:layout="sap.ui.layout"
>
    <m:FlexBox
        id="_IDGenFlexBox80"
        renderType="Bare"
        width="30rem"
        height="15rem"
    >
        <m:Image
            id="_IDGenImzeczeage1"
            src="../../../assets/donutchart.png"
            visible="{donutModel>/visibilityImg}"
            width="14rem"
            height="7rem"
            class="chartimg"
        />
        <InteractiveDonutChart
            id="_IDGenInteractiveDonutChart150"
            segments="{donutModel>/donutData}"
            visible="{donutModel>/visibilityDonut}"
        >
            <segments>
                <InteractiveDonutChartSegment
                    id="_IDGenInteractiveDonutChartSegment150"
                    label="{donutModel>label}"
                    value="{donutModel>value}"
                    displayedValue="{donutModel>displayedValue}"
                />
            </segments>
        </InteractiveDonutChart>
    </m:FlexBox>
</mvc:View>
