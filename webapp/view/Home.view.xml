<mvc:View
    controllerName="management.controller.Profile.ConsultantDetails"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns:tnt="sap.tnt"
    xmlns:f="sap.ui.layout.form"
    xmlns:a="sap.f"
    displayBlock="true"
>
    <ScrollContainer
        id="_IDGenScrollContainer1"
        vertical="true"
        horizontal="false"
        height="100%"
    >
        <a:Card
            id="_IDGenCard1"
            class="sapUiMediumMargin"
            width="95%"
        >
            <a:content>
                <VBox
                    id="_IDGenVBox1a"
                    class="sapUiSmallMargin"
                >
                    <f:Form
                        id="FormToolbar"
                        editable="true"
                        ariaLabelledBy="Title1"
                        width="100%"
                    >
                        <f:toolbar>
                            <Toolbar id="TB1">
                                <Title
                                    id="Title1"
                                    text="My profile "
                                />
                                <ToolbarSpacer id="_IDGenToolbarSpacer1" />
                                <tnt:InfoLabel
                                    id="il10dedz"
                                    text="{= ${Disponilbilty} === '0' ? 'Unavailable' : 'Available'}"
                                    colorScheme="{= ${Disponilbilty} === '0' ? 2 : 8}"
                                    displayOnly="true"
                                />
                            </Toolbar>
                        </f:toolbar>
                        <f:layout>
                            <f:ResponsiveGridLayout
                                id="_IDGenResponsiveGridLayout1"
                                labelSpanXL="0"
                                labelSpanL="0"
                                labelSpanM="0"
                                labelSpanS="0"
                                emptySpanXL="0"
                                emptySpanL="0"
                                emptySpanM="0"
                                emptySpanS="0"
                                columnsXL="3"
                                columnsL="3"
                                columnsM="3"
                                singleContainerFullSize="false"
                            />
                        </f:layout>
                        <f:formContainers>
                            <f:FormContainer
                                id="_IDGenFormContainer1"
                                ariaLabelledBy="Title2"
                            >
                                <f:toolbar>
                                    <Toolbar id="_IDGenToolbar1">
                                        <Title
                                            id="Title2"
                                            text="Personal Information"
                                        />
                                        <ToolbarSpacer
                                            id="_IDGenToolbarSpacer2"
                                        />
                                    </Toolbar>
                                </f:toolbar>
                                <f:formElements>
                                    <f:FormElement id="_IDGenFormElement1">
                                        <HBox
                                            id="_IDGenHBox1"
                                            width="90%"
                                            class="SapUiMediumMarginTop marginLeft"
                                        >
                                            <mvc:XMLView
                                                id="_IDGenXMLView1"
                                                viewName="management.view.Profile.Card"
                                            />
                                            <layoutData>
                                                <FlexItemData
                                                    id="_IDGenFlexItemData1"
                                                    growFactor="1"
                                                    baseSize="0"
                                                    backgroundDesign="Solid"
                                                    styleClass="sapUiTinyMargin"
                                                />
                                            </layoutData>
                                        </HBox>
                                    </f:FormElement>
                                </f:formElements>
                            </f:FormContainer>
                            <f:FormContainer
                                id="_IDGenFormContainer2"
                                ariaLabelledBy="Title3"
                            >
                                <f:toolbar>
                                    <Toolbar id="_IDGenToolbar2">
                                        <Title
                                            id="Title3"
                                            text="Availability Calendar"
                                        />
                                        <ToolbarSpacer
                                            id="_IDGenToolbarSpacer3"
                                        />
                                    </Toolbar>
                                </f:toolbar>
                                <f:formElements>
                                    <f:FormElement id="_IDGenFormElement2">
                                        <HBox
                                            id="_IDGenHBox2"
                                            width="100%"
                                            class="marginLeftCalendar"
                                        >
                                            <mvc:XMLView
                                                id="_IDGenXMLView2"
                                                viewName="management.view.Profile.Calendar"
                                            />
                                            <layoutData>
                                                <FlexItemData
                                                    id="_IDGenFlexItemData2"
                                                    growFactor="1"
                                                    baseSize="0"
                                                    backgroundDesign="Solid"
                                                    styleClass="sapUiTinyMargin"
                                                />
                                            </layoutData>
                                        </HBox>
                                    </f:FormElement>
                                </f:formElements>
                            </f:FormContainer>
                            <f:FormContainer
                                id="_IDGenFormContainer3"
                                ariaLabelledBy="Title4"
                            >
                                <f:toolbar>
                                    <Toolbar id="_IDGenToolbar3">
                                        <Title
                                            id="Title4"
                                            text="Tickets Stats"
                                        />
                                        <ToolbarSpacer
                                            id="_IDGenToolbarSpacer4"
                                        />
                                    </Toolbar>
                                </f:toolbar>
                                <f:formElements>
                                    <f:FormElement
                                        id="_IDGenFormElement3"
                                        class="marginLeft20"
                                    >
                                        <HBox
                                            id="_IDGenHBox3"
                                            width="100%"
                                            class="marginLeft20"
                                        >
                                            <mvc:XMLView
                                                id="_IDGenXMLView3"
                                                viewName="management.view.Profile.Donut"
                                                class="marginLeft20"
                                            />
                                            <layoutData>
                                                <FlexItemData
                                                    id="_IDGenFlexItemData3"
                                                    growFactor="1"
                                                    baseSize="0"
                                                    backgroundDesign="Solid"
                                                />
                                            </layoutData>
                                        </HBox>
                                    </f:FormElement>
                                    <f:FormElement
                                        id="_IDGenFormElementszed3"
                                        class="donutile"
                                    >
                                        <VBox
                                            id="_IDdedczcGenHBox3"
                                            width="100%"
                                            class="donutile"
                                        >
                                            <layoutData>
                                                <FlexItemData
                                                    id="_IDGenFlexItemDedcfata3"
                                                    growFactor="1"
                                                    baseSize="0"
                                                    backgroundDesign="Solid"
                                                />
                                            </layoutData>
                                            <GenericTile
                                                class="donutile"
                                                id="tileAssigned"
                                                header="Total Tickets"
                                                subheader="Done"
                                                press="onTicketTilePress"
                                            >
                                                <TileContent
                                                    id="_IDGenTileContent73"
                                                    footer="Total Number"
                                                >
                                                    <NumericContent
                                                        id="tileAssignedCount"
                                                        icon="sap-icon://message-success"
                                                        value="{doneTickets>/}"
                                                        withMargin="false"
                                                        class="success"
                                                    />
                                                </TileContent>
                                            </GenericTile>
                                        </VBox>
                                    </f:FormElement>
                                </f:formElements>
                            </f:FormContainer>
                        </f:formContainers>
                    </f:Form>
                    
                </VBox>
            </a:content>
        </a:Card>
        <mvc:XMLView
            id="_IDGenXMLView4"
            viewName="management.view.Profile.TicketsTable"
        />
    </ScrollContainer>
</mvc:View>
