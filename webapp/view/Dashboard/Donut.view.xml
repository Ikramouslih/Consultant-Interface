<mvc:View controllerName="management.controller.ConsultantDetails"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:m="sap.m"
    xmlns="sap.suite.ui.microchart"
    xmlns:layout="sap.ui.layout">
<m:FlexBox id="_IDGenFlexBox80"  renderType="Bare" width="30rem" height="15rem" class="paddingmedium">

    <InteractiveDonutChart id="_IDGenInteractiveDonutChart150"
        segments="{donutModel>/donutData}"
        selectionChanged="onSelectionChanged" class="smallerDonut">
        <segments  >
            <InteractiveDonutChartSegment id="_IDGenInteractiveDonutChartSegment150"
                label="{donutModel>label}"
                value="{donutModel>value}"
                displayedValue="{donutModel>displayedValue}" class="itemscontainer"/>
        </segments>
        
    </InteractiveDonutChart>
</m:FlexBox>

</mvc:View>