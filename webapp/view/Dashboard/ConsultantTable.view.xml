<mvc:HTMLView id="_IDGenHTMLView1"
              xmlns:mvc="sap.ui.core.mvc"
              controller="management.controller.ConsultantTable"
                xmlns="sap.m"
                xmlns:f="sap.f"
                xmlns:card="sap.f.cards"
                xmlns:core="sap.ui.core"
                displayBlock="true"
                height="100%">

    <Table id="idProductsTable" inset="false" items="{path: '/TICKETIDSet'}">
                <headerToolbar>
                    <Toolbar id="_IDGenToolbar1">
                        <ToolbarSpacer id="_IDGenToolbarSpacer1" />
                        <SearchField id="searchField" width="auto" search=".onFilterPosts" />
                    </Toolbar>
                </headerToolbar>
                <columns>
                    <Column id="_IDGenColumn1" width="12em">
                        <Text id="_IDGenText1" text="ID (Jira)" />
                    </Column>
                    <Column id="_IDGenColumn2" minScreenWidth="Tablet" demandPopin="true">
                        <Text id="_IDGenText2" text="Titre" />
                    </Column>
                    <Column id="_IDGenColumn3" minScreenWidth="Desktop" demandPopin="true" width="5em">
                        <Text id="_IDGenText3" text="Project" />
                    </Column>
                    <Column id="_IDGenColumn4"  hAlign="Center">
                        <Text id="_IDGenText8"  text="Created" />
                    </Column>
                    <Column id="_IDGenColumn5"  hAlign="Center">
                        <Text id="_IDGenText9"  text="Status" />
                    </Column>
                    <Column id="_IDGenColumn6"  hAlign="Center">
                        <Text id="_IDGenText10"  text="Technology" />
                    </Column>
                    <Column id="_IDGenColumn7"  hAlign="Center">
                        <Text id="_IDGenText11"  text="Days" />
                    </Column>
                    <Column id="_IDGenColumn8"  hAlign="Center">
                        <Text id="_IDGenText16"  text="Consultant" />
                    </Column>
                </columns>
                <items>
                    <ColumnListItem id="_IDGenColumnListItem1" vAlign="Middle">
                        <cells>
                            <Link id="_IDGenObjectIdentifier1" text="{IdJira}" class="idJiraColumn" press="showTicketInfo"/>
                            <Text id="_IDGenText7" text="{Titre}" />
                            <Text id="_IDGenText77" text="{Projet}" />
                            <Text id="_IDGenText15" text="{CreatedBy}" />
                            <Text id="_IDGenText12" text="{Technology}" />
                            <Text id="_IDGenText13" text="{Technology}" />
                            <Text id="_IDGenText14" text="{StartDate}" />
                            <Text id="_IDGenText17" text="{Consultant}" />
                        </cells>
                    </ColumnListItem>
                </items>
            </Table>
</mvc:HTMLView>