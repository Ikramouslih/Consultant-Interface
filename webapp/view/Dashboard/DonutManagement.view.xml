<mvc:View
    controllerName="management.controller.Donut"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:m="sap.m"
    xmlns="sap.suite.ui.microchart"
    xmlns:layout="sap.ui.layout"
>
    <InteractiveDonutChart
        id="_IDGenInteractiveDonutChart15"
        segments="{donutManagementData>/donutData}"
        selectionChanged="onSelectionChanged"
    >
        <segments>
            <InteractiveDonutChartSegment
                id="_IDGenInteractiveDonutChartSegment15"
                label="{donutManagementData>label}"
                value="{donutManagementData>value}"
                displayedValue="{donutManagementData>displayedValue}"
            />
        </segments>
    </InteractiveDonutChart>
</mvc:View>
