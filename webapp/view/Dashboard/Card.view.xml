<mvc:View controllerName="management.controller.ConsultantDetails"
      xmlns:mvc="sap.ui.core.mvc"
                xmlns="sap.m"
                xmlns:f="sap.f"
                xmlns:card="sap.f.cards"
                xmlns:core="sap.ui.core"
                displayBlock="true"
                height="100%">
    <f:Card id="_IDGenCard1" class="sapUiMediumMargin" width="300px">
        <f:content>
            <List id="_IDGenList1"
                class="sapUiSmallMarginBottom"
                showSeparators="None"
                >
                <CustomListItem id="_IDGenCustomListItem1">
                    <HBox id="_IDGenHBox1" alignItems="Center" justifyContent="SpaceBetween">
                        <VBox id="_IDGenVBox1" class="sapUiSmallMarginBegin sapUiSmallMarginTopBottom" >
                            <Title id="_IDGenTitle1" level="H3" text="First Name:" class="cardTitle"/>
                            <Text id="_IDGenText1" text="{FirstName}" />
                        </VBox>
                    </HBox>
                    <VBox id="_IDGenVBox2" class="sapUiSmallMarginBegin sapUiSmallMarginTopBottom" >
                            <Title id="_IDGenTitle2" level="H3" text="Last Name:" class="cardTitle"/>
                            <Text id="_IDGenText2" text="{Name}" />
                    </VBox>
                    <VBox id="_IDGenVBox3" class="sapUiSmallMarginBegin sapUiSmallMarginTopBottom" >
                            <Title id="_IDGenTitle3" level="H3" text="Grade:" class="cardTitle"/>
                            <Text id="_IDGenText3" text="Junior" />
                    </VBox>
                    <VBox id="_IDGenVBox4" class="sapUiSmallMarginBegin sapUiSmallMarginTopBottom" >
                            <Title id="_IDGenTitle4" level="H3" text="Active:" class="cardTitle"/>
                            <Text id="_IDGenText4" text="{Disponilbilty}" />
                    </VBox>
                    <VBox id="_IDGenVBox5" class="sapUiSmallMarginBegin sapUiSmallMarginTopBottom" >
                            <Title id="_IDGenTitle5" level="H3" text="UserName:" class="cardTitle"/>
                        <Text id="_IDGenText5" text="{Login}" />
                    </VBox>
                    <VBox id="_IDGenVBox6" class="sapUiSmallMarginBegin sapUiSmallMarginTopBottom" >
                            <Title id="_IDGenTitle6" level="H3" text="Mail:" class="cardTitle"/>
                            <Text id="_IDGenText6" text="{Email}" />
                    </VBox>
                </CustomListItem>
            </List>
        </f:content>
    </f:Card>
</mvc:View>
