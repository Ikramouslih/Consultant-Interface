<mvc:View xmlns:cards="sap.f.cards"
	controllerName="management.controller.TicketManagement.CTicket"
	xmlns:dnd="sap.ui.core.dnd"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:c="sap.ui.core"
	xmlns="sap.m"
	height="100%">
	
	<Page id="_IDGenPage1"
		showHeader="false"
		enableScrolling="true"
		class="sapUiContentPadding">
		<headerContent>
			
		</headerContent>
		<content>
			<HBox id="_IDGenHBox1" renderType="Bare">
				<mvc:XMLView id="OnHold" viewName="management.view.TicketManagement.CTOnHold" async="true"/>
				<VBox id="_IDGenVBox1" justifyContent="Center" class="sapUiTinyMarginBeginEnd">
					<Button id="_IDGenButton1"
						class="sapUiTinyMarginBottom"
						icon="sap-icon://navigation-right-arrow"
						tooltip="Move to In Progress"
						press="onMoveToInProgress"/>
					<Button id="_IDGenButton2"
						icon="sap-icon://navigation-left-arrow"
						tooltip="Move to On Hold"
						press="onMoveToOnHold"/>
				</VBox>
				<mvc:XMLView id="InProgress" viewName="management.view.TicketManagement.CTInProgress" async="true"/>
                <VBox id="_IDGenVBox2" justifyContent="Center" class="sapUiTinyMarginBeginEnd">
					<Button id="_IDGenButton3"
						class="sapUiTinyMarginBottom"
						icon="sap-icon://navigation-right-arrow"
						tooltip="Move to Done"
						press="onMoveToDone"/>
					<Button id="_IDGenButton4"
						icon="sap-icon://navigation-left-arrow"
						tooltip="Move to In progress"
						press="onMoveToInProgress2"/>
				</VBox>
				<mvc:XMLView id="Done" viewName="management.view.TicketManagement.CTDone" async="true"/>
			</HBox>
		</content>
	</Page>
</mvc:View>