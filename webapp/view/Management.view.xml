<mvc:View
    controllerName="management.controller.Management"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:grid="sap.ui.layout.cssgrid"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:smartchart="sap.ui.comp.smartchart"
    xmlns:viz="sap.viz.ui5.controls"
    xmlns:viz.data="sap.viz.ui5.data"
    xmlns:viz.feeds="sap.viz.ui5.controls.common.feeds"
    xmlns:formLayout="sap.ui.layout.form"

>
    <ScrollContainer
        id="_IDGenScrollContainer1"
        vertical="true"
        horizontal="false"
        height="100%"
    >
        <Panel
            id="panelForGridList"
            backgroundDesign="Transparent"
        >
            <headerToolbar>
                <Toolbar
                    id="_IDGenToolbar1"
                    height="3rem"
                >
                    <Title
                        id="_IDGenTitle1"
                        text="Dashboard"
                    />
                </Toolbar>
            </headerToolbar>

            <!-- **************************************** -->

            <mvc:XMLView
                id="_IDGenXMLView3"
                viewName="management.view.Dashboard.KeyNumbers"
                class="sapUiSmallMarginBottom"
            />

            <!-- **************************************** -->

            <mvc:XMLView
                id="_IDGenXMLView1"
                viewName="management.view.Dashboard.Tiles"
                class="sapUiSmallMarginBottom"
            />

            <!-- **************************************** -->

            <formLayout:Form id="FormToolbar" editable="true" ariaLabelledBy="Title1" class="sapMOHR sapMOHRBgSolid sapMOHR sapMOHRBgTranslucent">
                    <formLayout:layout>
                        <formLayout:ResponsiveGridLayout id="_IDGenResponsiveGridLayout1"
                            labelSpanXL="12"
                            labelSpanL="12"
                            labelSpanM="12"
                            labelSpanS="12"
                            emptySpanXL="0"
                            emptySpanL="0"
                            emptySpanM="0"
                            emptySpanS="0"
                            columnsXL="1"
                            columnsL="1"
                            columnsM="1"
                            singleContainerFullSize="false"
                        />
                    </formLayout:layout>
                    <formLayout:FormContainer id="_IDGenFormContainer2" ariaLabelledBy="Title3">
                        <formLayout:formElements>
                            <formLayout:FormElement id="_IDGenFormElement5">
                            <mvc:XMLView
                                    id="_IDGenXMLView3q3"
                                    viewName="management.view.Dashboard.Last30DaysTickets"
                                    class="sapUiSmallMarginBottom"
                                />
                            </formLayout:FormElement>
                        </formLayout:formElements>
                    </formLayout:FormContainer>
                    <formLayout:formContainers>           
                </formLayout:formContainers>
            </formLayout:Form>
            
        </Panel>
    </ScrollContainer>
</mvc:View>
